{
  "name": "agent-todo",
  "version": "1.0.0",
  "description": "MCP-based task queue for parallel agent workflows",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-todo-server": "./dist/mcp-server.js",
    "agent-todo-worker": "./dist/worker.js"
  },
  "scripts": {
